{
    "taskDefinitionArn": "arn:aws:ecs:eu-north-1:736187710965:task-definition/budget:latest",
    "containerDefinitions": [
        {
            "name": "budget_rabbitmq",
            "image": "736187710965.dkr.ecr.eu-north-1.amazonaws.com/budget_rabbitmq:latest",
            "cpu": 0,
            "portMappings": [
                {
                    "name": "budget_rabbitmq-5672-tcp",
                    "containerPort": 5672,
                    "hostPort": 5672,
                    "protocol": "tcp",
                    "appProtocol": "http"
                },
                {
                    "name": "budget_rabbitmq-15672-tcp",
                    "containerPort": 15672,
                    "hostPort": 15672,
                    "protocol": "tcp",
                    "appProtocol": "http"
                }
            ],
            "essential": true
        },
        {
            "name": "budget_server",
            "image": "736187710965.dkr.ecr.eu-north-1.amazonaws.com/budget_server:latest",
            "cpu": 0,
            "portMappings": [
                {
                    "name": "budget_server-8080-tcp",
                    "containerPort": 8080,
                    "hostPort": 8080,
                    "protocol": "tcp",
                    "appProtocol": "http"
                }
            ],
            "essential": true
        },
        {
            "name": "budget_client",
            "image": "736187710965.dkr.ecr.eu-north-1.amazonaws.com/budget_client:latest",
            "cpu": 0,
            "portMappings": [
                {
                    "name": "budget_client-3000-tcp",
                    "containerPort": 3000,
                    "hostPort": 3000,
                    "protocol": "tcp",
                    "appProtocol": "http"
                }
            ],
            "essential": true
        }
    ],
    "family": "budget",
    "executionRoleArn": "arn:aws:iam::736187710965:role/ecsTaskExecutionRole",
    "networkMode": "awsvpc",
    "requiresCompatibilities": [
        "FARGATE"
    ],
    "cpu": "1024",
    "memory": "3072",
    "runtimePlatform": {
        "cpuArchitecture": "X86_64",
        "operatingSystemFamily": "LINUX"
    }
}